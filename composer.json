{
    "name": "jasny/http-message",
    "type": "library",
    "description": "PSR-7 implementation for handling HTTP requests",
    "keywords": ["psr-7", "http", "mvc"],
    "license": "MIT",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        }
    ],
    "support": {
        "issues": "https://github.com/jasny/meta/issues",
        "source": "https://github.com/jasny/meta"
    },
    "require": {
        "php": ">=5.6.0",
        "psr/http-message": "^1.0",
        "jasny/php-functions": "^2.2|^3.0"
    },
    "require-dev": {
        "jasny/php-code-quality": "^2.1",
        "mikey179/vfsStream": "^1.6"
    },
    "provide": {
        "psr/http-message-implementation": "~1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Jasny\\HttpMessage\\": "src/"
        }
    },
    "autoload-dev": {
